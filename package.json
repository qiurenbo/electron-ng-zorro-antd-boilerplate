{
  "name": "electron-ng-zorro-antd-boilerplate",
  "version": "0.0.0",
  "scripts": {
    "start": "concurrently -c \"yellow.bold,green.bold\" -n \"NG,ELECTRON\" \"ng serve\" \"npm run electron:serve\"",
    "electron:serve": "wait-on tcp:4200 && npm run main:transpile:dev && npx electron . --serve",
    "main:transpile:dev": "tsc -p tsconfig-electron.dev.json",
    "main:transpile:prod": "tsc -p tsconfig-electron.prod.json",
    "electron:build": "npm run ng:build -- --prod && npm run main:transpile:prod && electron-builder build",
    "ng:build": "ng build --base-href ./",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "main": "main.js",
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.11",
    "@angular/common": "~10.0.11",
    "@angular/compiler": "~10.0.11",
    "@angular/core": "~10.0.11",
    "@angular/forms": "~10.0.11",
    "@angular/platform-browser": "~10.0.11",
    "@angular/platform-browser-dynamic": "~10.0.11",
    "@angular/router": "~10.0.11",
    "ng-zorro-antd": "^9.3.0",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "wait-on": "^5.2.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.7",
    "@angular/cli": "~10.0.7",
    "@angular/compiler-cli": "~10.0.11",
    "@schematics/angular": "^9.1.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "concurrently": "^5.3.0",
    "electron": "^10.0.0",
    "electron-builder": "^22.8.0",
    "electron-reload": "^1.5.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5"
  }
}
